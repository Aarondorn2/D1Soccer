
{{#content-container}}

  <div class="row" style = "padding: 20px 40px 20px 40px">
     {{#each groupedImages as |row|}}
        <div class="row">
          {{#each row as |image|}}
            {{image-thumbnail image=image}}
          {{/each}}
        </div>
      {{/each}}
   </div>

{{/content-container}}
